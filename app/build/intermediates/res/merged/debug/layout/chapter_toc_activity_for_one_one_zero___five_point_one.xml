<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="8dp"
    android:paddingLeft="8dp"
    android:paddingRight="8dp"
    android:paddingTop="8dp"
    tools:context=".MainActivity" >
 
    <TextView
        android:id="@+id/my_textview"
        android:layout_width="368dip"
        android:layout_height="52dip"
        android:background="@drawable/text_continuity_button"
        android:paddingTop="6dp" 
        android:textSize="7pt"
        android:textColor="#FFFFFF"/> 
        

    
    <TextView
        android:id="@+id/my_top_underscore"
        android:layout_width="368dip"
        android:layout_height="2dip"
        android:background="@drawable/text_continuity_button"
        android:layout_marginTop="54dp"
        /> 

    <WebView
        android:id="@+id/my_webview"
        android:paddingTop="0dip"
        android:layout_width="368dip"
        android:layout_height="454dip"
        android:layout_marginTop="58dip"
        android:layout_marginLeft="0dip"
        android:layout_marginRight="0dip"
        android:paddingBottom="0dip"  
        android:textColorLink="#335495"
        /> 

    <ImageView
        android:id="@+id/footbar0"
        android:layout_width="368dip"
        android:layout_height="34dip"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="2dip"
        android:layout_marginLeft="0dip"
        android:layout_marginRight="2dip"
        android:layout_marginTop="8dip"
        android:background="@drawable/text_continuity_button"
        android:onClick="goToPreviousSection"
        android:src="@drawable/arrow_light_blue_xxsmall_left" />
    
