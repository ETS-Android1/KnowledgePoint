<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"   
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >
 
    <TextView
        android:id="@+id/my_textview"
        android:layout_width="460dip" 
        android:layout_height="58dip"
        android:background="@drawable/text_continuity_button"
        android:paddingTop="6dp" 
        android:textSize="7pt"
        android:textColor="#FFFFFF"/> 
        
    <ImageView
        android:id="@+id/contents_button"
        android:layout_marginLeft="460dp"
        android:layout_width="120dip"
        android:layout_height="58dip"
        android:background="@drawable/text_continuity_button"
        android:onClick="showPopUpMenu"
        android:src="@drawable/arrow_light_blue_xxsmall_down" />
    
        <TextView
        android:id="@+id/my_top_underscore"
        android:layout_width="580dip" 
        android:layout_height="2dip"
        android:background="@drawable/text_continuity_button"
        android:layout_marginTop="60dp"
        /> 
        
    <LinearLayout  
        android:layout_marginTop="0dip"
		android:orientation="horizontal"  
		android:layout_width="580dip"  
		android:layout_height="58dip"  
		android:paddingBottom="0dip"
		android:id="@+id/searchbox"  
  		>
  		
    </LinearLayout>  

    <WebView
        android:id="@+id/my_webview"
        android:paddingTop="0dip"
        android:layout_width="580dip"
        android:layout_height="682dip"
        android:layout_marginTop="64dip"
        android:layout_marginLeft="0dip"
        android:layout_marginRight="0dip"
        android:paddingBottom="0dip" 
        android:textSize="16dip"
        /> 

    <ImageView
        android:id="@+id/footbar0"
        android:layout_width="190dip"
        android:layout_height="44dip"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="2dip"
        android:layout_marginLeft="0dip"
        android:layout_marginRight="0dip"
        android:layout_marginTop="8dip"
        android:background="@drawable/text_continuity_button"
        android:onClick="goToPreviousSection"
        android:src="@drawable/arrow_light_blue_xxsmall_left" />

    <ImageView
        android:id="@+id/footbar1"
        android:layout_width="190dip"
        android:layout_height="44dip"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="2dip"
        android:layout_marginLeft="0dip"
        android:layout_marginTop="0dip"
        android:background="@drawable/text_continuity_button"
        android:onClick="returnToMenu"
        android:src="@drawable/arrow_light_blue_xxsmall_up" />

    <ImageView
        android:id="@+id/footbar2"
        android:layout_width="190dip"
        android:layout_height="44dip"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="2dip"
        android:layout_marginLeft="0dip"
        android:layout_marginRight="0dip"
        android:layout_marginTop="0dip"
        android:background="@drawable/text_continuity_button"
        android:onClick="goToNextSection"
        android:src="@drawable/arrow_light_blue_xxsmall_right" />

</RelativeLayout><!-- From: file:/C:/Users/tony.hillman/Documents/AndroidStudioBackups/MaterialWithTabs/V01/UAKP-all-22-1-21-g6/app/src/main/res/layout/chapter_activity_for_one_zero_zero___seven.xml -->